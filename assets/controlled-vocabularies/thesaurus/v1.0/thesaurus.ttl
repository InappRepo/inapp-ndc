  rdfs:label "Principle of subsidiarity (Fonte: Eurovoc)"@en, "Principio di sussidiarietà"@it ;
  dc:type "Termine formale", "ACC", "Voce generica" ;
  dc:modified "45345.0"^^xsd:date .

